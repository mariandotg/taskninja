{
  "name": "nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "next lint",
    "create-databases": "node createLocalDatabases.js",
    "migrate:generate": "yarn drizzle-kit generate:sqlite",
    "dev/migrate:push": "tsx src/lib/db/migrate.mjs",
    "test/migrate:push": "cross-env NODE_ENV=test tsx src/lib/db/migrate.mjs",
    "prod/migrate:push": "cross-env NODE_ENV=production tsx src/lib/db/migrate.mjs",
    "migrate": "yarn run migrate:generate && yarn run test/migrate:push && yarn run dev/migrate:push"
  },
  "dependencies": {
    "@libsql/client": "^0.5.2",
    "@types/node": "20.4.9",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "10.4.14",
    "drizzle-orm": "^0.29.4",
    "eslint": "^8.46.0",
    "eslint-config-next": "^14.1.1",
    "next": "^14.1.1",
    "next-auth": "^4.24.6",
    "postcss": "8.4.27",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-jest": "^29.6.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.14",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "^2.8.8",
    "tsx": "^4.7.1"
  }
}
